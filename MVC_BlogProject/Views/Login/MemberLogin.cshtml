
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Sign In</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/AdminLTE-3.0.4/login/Login_v15/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/AdminLTE-3.0.4/login/Login_v15/css/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="login100-form-title" style="background-image: url(/AdminLTE-3.0.4/login/Login_v15/images/movie-background.jpg)">
                    <span class="login100-form-title-1">
                        Sign In
                    </span>
                </div>
                <form action="/Login/MemberLogin/" method="post" class="login100-form validate-form">
                    <br />
                   
                    <div class="wrap-input100 validate-input m-b-26" data-validate="Username is required">
                        <span class="label-input100">Mail Adresiniz:</span>
                        <input class="input100" type="email" name="MemberMail" placeholder="">
                        <span class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100 validate-input m-b-18" data-validate="Password is required">
                        <span class="label-input100">Password</span>
                        <input class="input100" type="password" name="MemberPassword" placeholder="">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="flex-sb-m w-full p-b-30">
                        <div class="contact100-form-checkbox">
                            <input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
                            <label class="label-checkbox100" for="ckb1">
                                Remember me
                            </label>
                        </div>
                        <div>
                            <a href="/Login/SignUp" class="txt1" style="font-size:15px;">
                                <b>ÜYE OL</b>
                            </a>
                        </div>
                        <div>
                            <a href="/Home/Index" class="txt1">
                                İptal
                            </a>
                        </div>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                           Giriş Yap
                        </button>

                    </div>
             
                </form>
            </div>
        </div>
    </div>

    <!--===============================================================================================-->
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/bootstrap/js/popper.js"></script>
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminLTE-3.0.4/login/Login_v15/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/AdminLTE-3.0.4/login/Login_v15/js/main.js"></script>

</body>
</html>



























